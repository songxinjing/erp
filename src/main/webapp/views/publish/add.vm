//物品属性名称检查
function checkItemSpecisName(val,type){
	var isSame = false;
	var title = type == 1 ? '自定义物品属性名称检测' : '商品属性名称检测';
	var text = type == 1 ? '该物品属性名称【'+val+'】已存在!' : '该商品属性名称【'+val+'】已存在';
	if(itemSpecisNameArray.length > 0 ){
		for(var i in itemSpecisNameArray){
			if(itemSpecisNameArray[i] == val.toLowerCase()){
				$.gritter.add({
					class_name:'gritter-error',
					title: title,
					text: text,
					time: 2000,
					sticky: false
				});
				return true;
			}
		}
	}
	itemSpecisNameArray.push(val);
	return isSame;
}

