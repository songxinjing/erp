var sourceMultiArray = new Array();
function saveMultiData(arr){
    $("#multi-list tbody tr").each(function(){
    	var label = $(this).attr("data-target") || "";
    	var sku =  $(this).children(".field-sku").children("input.form-control").val();
    	var stock =  $(this).children(".field-stock").children("input.form-control").val();
    	var price =  $(this).children(".field-price").find("input.form-control").val();
    	var ean =  $(this).children(".field-ean").children("input.form-control").val();
    	var imageFile = new Array();
    	for(var i=0; i< $(this).find(".magnific").length; i++){
    		imageFile.push($(this).find(".magnific input[type='hidden']").eq(i).val());
    	};
    	var newData = [label,sku,stock,price,ean,imageFile];
    	var flag = false;
    	for(var j=0; j< arr.length; j++){
    		if(arr[j][0] == label){
    			flag = true;
    			arr[j] = newData;
    			break;
    		}
    	}
    	if(!flag) arr.push(newData);
    });
};
