//图片复制到所有多属性
$("#multi-list").on("click",".imageCopy",function(){
	var uploadimg = $(this).parents("tr").find(".uploadimg");
	var cAttrV = $(this).parents("tr").attr('data-target');
	
	var iAttr=$("input[name='imageRelate']:checked").val();
	var iAttrV=[];
	var attrVal='';

	
	$("input[name='"+iAttr+"'][checked]").each(function(){
		iAttrV.push($(this).val());
		if(cAttrV.indexOf($(this).val()) >= 0){
			attrVal=$(this).val();
		}
	});

	
	if(uploadimg.length > 0 && $("#multi-list tbody tr").length > 1){
		var imageUrlArr = [];
		uploadimg.each(function(){
			imageUrlArr.push($(this).attr("href"));
		});
		var dataTarget = "";
		$(this).parents("tr").siblings("tr").each(function(){
			dataTarget = $(this).attr("data-target");
			if(attrVal && dataTarget.indexOf(attrVal) < 0){
				return true;
				}
			var  magnific="";
			for(var i=0; i<imageUrlArr.length; i++){
				magnific += '<a title="点击放大/缩小" href="'+ imageUrlArr[i] +'" class="uploadimg magnific">'+
		    				'<span><img src="'+ imageUrlArr[i] +'"><em title="删除" class="remove ico-close2"></em></span>'+
		    				'<input type="hidden" value="'+ imageUrlArr[i] +'" name="'+ dataTarget +'_imageFile[]">'+
		    			'</a>';
			};
			$(this).children(".field-url").find(".uploadimg").remove();
			$(this).children(".field-url").append(magnific);
// 			$(this).children(".field-url").find(".currentNum").text(uploadimg.length);
		});
	}
});

