								sticky: false
							});
							return ;
						}
					}

					if(checkItemSpecisName(result,3)){
						$("#tempInput").val('');return;
					}
					
					if(customItemSpecisNameArray.length > 0 && $.inArray(oldName.toLowerCase(),customItemSpecisNameArray) > 0){
