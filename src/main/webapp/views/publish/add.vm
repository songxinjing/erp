    $("#import-template input.form-control").on("keyup",function(){
        var val=$.trim($(this).val()).toLowerCase();
        var sortName=$("#import-template table.list-body tr");
        if(val.length==0){
            sortName.show();
        }else{
            sortName.each(function(){
                $(this).find("td>span").first().text().toLowerCase().indexOf(val)>-1 ? $(this).show() : $(this).hide();
            });
        }
    });


    function getPublishTemplate(){
        var obj = $("#import-template .list-body tbody");
        $.ajax({
            type : "GET",
            url :"http://publish.mabangerp.com/index.php?m=ebay&a=getPublishTemplate",
            data : {shopId:$("#import-template input[name=shopId]:checked").val()},
            beforeSend:function(){ $('.loading_large').show();},
            dataType:"JSON",
            success:function(data){
                $('.loading_large').hide();
                obj.html('');
                if (data.html) {
                    obj.html(data.html);
                }else{
                    obj.html('<tr><td rowspan="3" align="center"><span  class="bold text-accent" >'+$("#import-template input[name=shopId]:checked").next().text()+'店铺,你还未添加任何模板 !</span></td></tr>');
                }
            }
        });
    }
    $("#import-ebay-template").on('click',function(){
        $("#import-template").show();
        getPublishTemplate();
    })

    $("#import-template button[data-dismiss='modal']").on('click',function(){
        $("#import-template").hide();
    });

    function doSaveTemplate(){
        if(!$("input[name='shopId']:checked").val()){
            showerror('请选择店铺',$("input[name=shopId]"));return'';
        }
        $("#publish-template-name").show();
    }

    $("#publish-template-name").on('click','.bootbox-close-button,.btn-default',function(){
        $("#publish-template-name").hide();
    });

    $("#import-template input[name='shopId']").on('click',function(){
        getPublishTemplate();
    })

    $("#publish-template-name button[data-bb-handler='success']").on('click',function(){
        var templateName = $("#publish-template-name input[name='templateName']").val();
        if(templateName){
            doSave(3,templateName);
        }else{
            $.gritter.add({
                class_name:'gritter-error',
                title: "刊登模板",
                text: "请输入模板名称",
                time: 2000,
                sticky: false
            });
        }
    })
    $("#import-template").on('click','.btn-remove',function(){
        var obj = $(this);
        bootbox.dialog({
            message: '<span class="text-danger fsize14 bold pl5 pr5">确认删模板吗？删除后不可还原！</span>',
            title: '删除模板操作',
            buttons: {
                success: {
                    label: "确认",
                    className: 'btn-danger',
                    callback: function () {
                        $.ajax({
                            url:'http://publish.mabangerp.com/index.php?m=ebay&a=deleteTemplate',
                            type:"post",
                            dataType:"json",
                            data:"&templateId="+obj.attr('data-template'),
                            success:function(r){
                                if(r.success){
                                    obj.parent().parent().remove();
                                }else{
                                    $.gritter.add({
