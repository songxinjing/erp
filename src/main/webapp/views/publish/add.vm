		$(".btn-savetemplate").on("click",function(){
			for ( instance in CKEDITOR.instances ) CKEDITOR.instances[instance].updateElement(); 
			var menuHeader = $(this).next(".load-template").find(".dropdown-header");
			var nameArr = [];
			menuHeader.nextAll().each(function(){
				nameArr.push($.trim($(this).children("a").text()));
			});

			var tempType=$(this).attr('data-attr');
			bootbox.dialog({
				title: "输入模板名称",
				message: '<input value="" class="form-control" id="tempInput" type="text">',				
				buttons: {
					success: {
						label: "确认",
						className: "btn-primary",
						callback: function () {
							var val = $("#tempInput").val();
							//名称未填写
							if(!$.trim(val)){
								$.gritter.add({
									class_name:'gritter-error',
									title: "不能保存",
									text: "请输入模板名称",
									time: 2000,
									sticky: false
								});
								$("#tempInput").focus();
								return false;
							};
							
							//检查重名
							if($.trim(nameArr[0])){
								var flag = false;
								for(var i=0;i<nameArr.length;i++){
									if(nameArr[i] == $.trim(val)){
										flag = true;
										break;
									}
								}
							};
							if(flag){
								$.gritter.add({
									class_name:'gritter-error',
									title: "名称重复",
									text: "模板名称不能重复，请重新命名",
									time: 2000,
									sticky: false
								});
								$("#tempInput").focus();
								return false;
							}

							var siteId=$("#siteId").val();
							//请求服务端 
							$.ajax({
								type : "post",
								url :"http://publish.mabangerp.com/index.php?m=template&a=addTemp",
								data :$("#ebay-form-detail").serialize()+"&siteId="+siteId+"&name="+val+"&tempType="+tempType,
								dataType:"JSON",
								beforeSend:function(){ $('.loading_large').show();},
								success:function(data){
								//保存成功
								$('.loading_large').hide();
									if(data.success){
										$.gritter.add({
											class_name:'gritter-success',
											title: "保存成功",
											text: "模板已保存",
											time: 2000,
											sticky: false
										});
										menuHeader.after('<li><a href="javascript:void(0);" onclick="showLogistic('+data.tempId+','+tempType+')" data-attr="'+data.tempId+'" tempType="'+tempType+'">'+ $.trim(val) +'</a><button class="close" type="button">×</button></li>');
									}else{
										$.gritter.add({
											class_name:'gritter-error',
											title: "提示",
											text: data.message,
											time: 2000,
											sticky: false
										});
									}
								},
							});
							
						}
					},
					cancel: {
						label: "取消",
						className: "btn-default",
						callback: function () {
							return null;
						}
					}
				}
			});
		});
		
