	//循环上传图片，引用共用upload.js中的上传方法
	var baseUrl="http://publish.mabangerp.com/index.php?m=image&a=doUpload";
	function uploadFile(fileList,postName,obj,pos){
		var i=1;
		$.each(fileList,function(k,v){
			uploadFileSingle(baseUrl,v,'1',i,postName,obj,pos);
			i++;
		});
	}
	
	function uploadFileSingle(baseUrl,file,platForm,num,postName,obj,pos){
		var fd = new FormData();
		fd.append(postName, file);
		fd.append("num", num);
		fd.append("platFormId", 1);
		fd.append("postName", postName);
		var xhr = new XMLHttpRequest();
		//xhr.setRequestHeader("Access-Control-Allow-Origin:*");
		xhr.upload.addEventListener("progress",	 function(e){onProgress(file.name, e.loaded, e.total);}, false);
		// 完成
		xhr.addEventListener("load", function(e){onSuccess(file.name, xhr.responseText,obj,pos);}, false);
		// 错误
		xhr.addEventListener("error", function(e){onFailure(file.name, xhr.responseText);}, false);
		xhr.open("POST",baseUrl,true);
		xhr.send(fd);
	}


