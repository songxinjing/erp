		var keywords=["&lt;script&gt;","&lt;/script&gt;","&lt;object&gt;","&lt;/object&gt;","&lt;EMBED","&lt;/EMBED&gt;","&lt;form&gt;","plug-in",'flash'];  
		function _filter_method(obj){  
		    //获取文本输入框中的内容  
		    var value = $(obj).val();
		    var isFilter=true;
		    var msg='';
		    //遍历敏感词数组  
		    for(var i=0;i<keywords.length;i++){  
		        //全局替换
		        var reg = new RegExp(keywords[i],"g");  
		        //判断内容中是否包括敏感词  
// 		        alert(value.indexOf(keywords[i]));
// 		        alert(value);
		        if(value.indexOf(keywords[i])!=-1){  
		        	isFilter=false;
		        	msg=keywords[i];
		        	break;
		        }
		    }
		    
		    res=new Object();
		    if(isFilter==true){
		    	res.success=true;
		    	res.msg='';
			 }else{
				 res.success=false;
			     res.msg=msg;
			 }
		    return res;
		}
		
		
