<div class="row">
	<div class="col-md-12">
		<div class="content-box-header">
			<div class="panel-title">我的店铺</div>
		</div>
		<div class="content-box-large box-with-header">
			<table class="table table-striped">
				              <thead>
				                <tr>
				                  <th>产品ID</th>
				                  <th>标题</th>
				                  <th>SKU</th>
				                </tr>
				              </thead>
				              <tbody>
									#if(!$!activeList || $!{activeList.size()} == 0)
										<tr>
											<td colspan="3">
												<h3>该店铺还没有任何上架产品！</h3>
											</td>
										</tr>
									#end 
									#foreach($item in $!activeList)
								        <tr>
								          <td>$!{item.itemID}</td>
								          <td>$!{item.title}</td>
								          <td>$!{item.sku}</td>
								        </tr>
									#end
				              </tbody>
				            </table>	  			
		</div>
	</div>
</div>


